for a in range(1,200): #От единицы, тк на ноль делить нельзя
  f=0
  for x in range(1,1001): #Если в условии сказано при любом натуральном x, то начинает с 1, иначе с 0
    if (((x%34==0) and (x%51!=0))<=((x%a!=0) or (x%51==0)))==False: #Переписывает уравнение из условия
      f=1
      break
    if f==0:
      print(a)
      break
